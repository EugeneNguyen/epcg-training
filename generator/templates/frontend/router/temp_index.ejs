import React from "react";
import {Switch, Route} from "react-router-dom";
import Table<%= modelNameCapitalCamel %>List from '../table_list';
import Form<%= modelNameCapitalCamel %>Add from '../form_add';
import Form<%= modelNameCapitalCamel %>Edit from '../form_edit';
import View<%= modelNameCapitalCamel %>Info from '../view_info';

export default function Route<%= modelNameCapitalCamel %>() {
  return (
    <Switch>
      <Route path="/<%= baseUrl %>" exact>
        <Table<%= modelNameCapitalCamel %>List />
      </Route>
      <Route path="/<%= baseUrl %>/add" exact>
        <Form<%= modelNameCapitalCamel %>Add />
      </Route>
      <Route path="/<%= baseUrl %>/:id" exact>
        <View<%= modelNameCapitalCamel %>Info />
      </Route>
      <Route path="/<%= baseUrl %>/:id/edit" exact>
        <Form<%= modelNameCapitalCamel %>Edit />
      </Route>
    </Switch>
  );
}